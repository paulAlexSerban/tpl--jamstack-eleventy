<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  
  <% 
  css.forEach(file => { 
    for (const [key, value] of Object.entries(file)) { %>
      <%- include('stylesheets', {type: key, file: value}); %>
  <% }}); %>

  <title><%= title %></title>
</head>

